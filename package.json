    "archiver": "1.0.1",
    "async": "1.5.2",
    "bindings": "1.2.1",
    "co": "4.6.0",
    "colors": "1.1.2",
    "commander": "2.9.0",
    "daemonize2": "0.4.2",
    "duniter-bma": "^0.2.1",
    "duniter-common": "0.1.0",
    "duniter-crawler": "^0.2.2",

{
  "name": "duniter",
  "version": "1.6.14",
  "engines": {
    "node": ">=8.2.1 <9",
    "npm": ">=3.10"
  },
  "engineStrict": true,
  "private": false,
  "description": "Crypto-currency software allowing to build P2P free currencies",
  "main": "index.js",
  "node-main": "./bin/duniter",
  "window": {
    "icon": "duniter.png",
    "title": "v1.6.14",
    "width": 800,
    "height": 800,
    "min_width": 750,
    "min_height": 400
  },
  "directories": {
    "test": "test"
  },
  "scripts": {
    "prepublish": "tsc",
    "tsc": "tsc",
    "tscw": "tsc -w",
    "test": "nyc --reporter html mocha",
    "start": "node bin/duniter start",
    "build": "tsc && cd \"node_modules/duniter-ui\" && npm install && npm run build",
    "test-travis": "nyc --reporter lcovonly mocha test/"
  },
  "nyc": {
    "require": [
      "ts-node/register"
    ],
    "reporter": [
      "html"
    ],
    "sourceMap": true,
    "instrument": true
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:duniter/duniter.git"
  },
  "keywords": [
    "duniter",
    "ucoin",
    "openudc",
    "crypto-currency"
  ],
  "author": "Cedric Moreau <cem.moreau@gmail.com>",
  "license": "AGPL-3.0",
  "readmeFilename": "README.md",
  "bugs": {
    "url": "https://github.com/duniter/duniter/issues"
  },
  "dependencies": {
    "archiver": "1.3.0",
    "async": "2.2.0",
    "bindings": "1.2.1",
    "body-parser": "1.17.1",
    "bs58": "^4.0.1",
    "co": "4.6.0",
    "colors": "1.1.2",
    "commander": "2.9.0",
    "cors": "2.8.2",
    "daemonize2": "0.4.2",
    "ddos": "0.1.16",
    "errorhandler": "1.5.0",
    "event-stream": "3.3.4",
    "express": "4.15.2",
    "express-fileupload": "0.0.5",
    "inquirer": "3.0.6",
    "jison": "0.4.17",
    "js-yaml": "3.8.2",
    "merkle": "0.5.1",
    "moment": "2.18.1",
    "morgan": "1.8.1",
    "multimeter": "0.1.1",
    "naclb": "1.3.9",
    "nnupnp": "1.0.2",
    "node-pre-gyp": "0.6.34",
    "node-uuid": "1.4.8",
    "optimist": "0.6.1",
    "q-io": "1.13.2",
    "querablep": "^0.1.0",
    "request": "2.81.0",
    "request-promise": "4.2.0",
    "scryptb": "6.0.4",
    "seedrandom": "^2.4.3",
    "sha1": "1.1.1",
    "socks-proxy-agent": "^3.0.1",
    "sqlite3": "3.1.4",
    "tail": "^1.2.1",
    "tweetnacl": "0.14.3",
    "underscore": "1.8.3",
    "unzip": "0.1.11",
    "unzip2": "0.2.5",
    "winston": "2.3.1",
    "wotb": "0.6.x",
    "ws": "1.1.5"
  },
  "devDependencies": {
    "@types/mocha": "^2.2.41",
    "@types/node": "^8.0.9",
    "@types/should": "^8.3.0",
    "coveralls": "2.11.4",
    "eslint": "3.13.1",
    "eslint-plugin-mocha": "4.8.0",
    "mocha": "^3.4.2",
    "mocha-eslint": "0.1.7",
    "nyc": "^11.0.3",
    "sha1": "",
    "should": "*",
    "source-map-support": "^0.4.15",
    "supertest": "",
    "tmp": "0.0.29",
    "ts-node": "^3.3.0",
    "typescript": "^2.4.1"
  },
  "peerDependencies": {},
  "bin": {
    "duniter": "./bin/duniter"
  }
}

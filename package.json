    "optimist": "0.6.1",
    "parallelshell": "2.0.0",
    "q": "1.4.1",
    "q-io": "1.13.2",
    "querablep": "^0.1.0",
    "request": "2.75.0",
    "request-promise": "4.1.1",
    "sha1": "1.1.1",
    "sqlite3": "3.1.4",
